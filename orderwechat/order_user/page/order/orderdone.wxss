/* pages/order/detail/detail.wxss */
page{
  background:#F8F8F8
}
.ordertitle{
  background: white; 
  margin-top:10px;
  font-size:24px;
  padding: 12px;
  font-weight: 600;
  color: black;
  text-align: center;
  font-family: Cursive;
  /* border-bottom: 1px #E3E3E3 solid */
}
.order-info{
  background: white;
  /* margin-top:10px; */
}
.order-info-title{
  font-size:12px;
  color: #D1D1D1;
  padding: 12px;
  border-bottom: 1px #E3E3E3 solid
}
.order-sum{
  height:30px;
  padding:5px 15px;
  font-size:14px;
}
.order-sum-number{
  font-size:14px;
  float:right;
}
.list-info{
  width:50%;
  padding:5px 15px;
}
.list-info-size{
  font-size:12px;
  color:#B1B1B1;
}
.activity-color{
  color:#FF9C35;
}
.cart-list-box{
  background:#FFFFFF;
  display:flex;
  font-size:15px;
  border-bottom:1px #E3E3E3 solid;
}
.note-exchange{
  padding:3px 5px;
  border:1px solid #FF9C35;
  color:#FF9C35;
  margin-left: 140rpx;
  font-size:16px
}
.order-info-li{
  color:black;
  margin-left:10px;
}
.note-card{
  margin-left:20px;
  color:#A2A1A0;
  font-size:12px;
  margin-top:15px
}
.statu-card{
  font-size:12px;
  color:#FF9C35
}
/* 再来一单 */
button.moreorder{
  position: relative;
  width: 180rpx;
  height: 65rpx;
  border: 1px solid #FF9C35;
  background: #ffffff;
  border-radius: 0.7;
  font-size: 13px;
  margin-right: 5rpx;
}
.navmoreorder{
  width: 180rpx;
  height: auto;
  position: relative;
  margin-left: auto;
  margin-right: 5rpx;
}